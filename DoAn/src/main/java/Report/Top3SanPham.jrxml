	<queryString>
		<![CDATA[SELECT SP.TENSP, SUM(CTHD.SL) AS SL FROM CTHOADON CTHD JOIN SANPHAM SP ON SP.MASP = CTHD.MASP
                                                        JOIN HOADON HD ON HD.MAHD = CTHD.MAHD
WHERE EXTRACT (MONTH FROM NGAYHD) = TO_CHAR(SYSDATE, 'MM') and EXTRACT (YEAR FROM NGAYHD) = TO_CHAR(SYSDATE, 'YYYY')
GROUP BY SP.TENSP
ORDER BY SL DESC
FETCH FIRST 3 ROWS WITH TIES]]>
	</queryString>
